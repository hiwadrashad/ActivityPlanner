<style>
    @@keyframes slideInFromLeft {
        0% {
            transform: translateX(-100%);
        }

        100% {
            transform: translateX(0);
        }
    }

    body {
    overflow-x: hidden;
    }

    .slidein {
        /* This section calls the slideInFromLeft animation we defined above */
        animation: 1s ease-out 0s 1 slideInFromLeft;
        /*        background: #333;
    */ padding: 30px;
