    .scalethis {
/**/        transform: scale(1.05);
/**/    }
    .card__btn {
        display: block;
        width: 100%;
        padding: 1.5rem;
        font-size: 2rem;
        text-align: center;
        color: #3363ff;
        background-color: #e6ecff;
        border: none;
        border-radius: 0.4rem;
        transition: 0.2s;
        cursor: pointer;
        transform: scale(0.5);
    }

        .card__btn span {
            margin-left: 1rem;
            transition: 0.2s;
        }

        .card__btn:hover, .card__btn:active {
            background-color: #dce4ff;
        }

            .card__btn:hover span, .card__btn:active span {
                margin-left: 1.5rem;
            }
</style>
